[user]
	name = Stephen Layland 
	email = slayland@tubi.tv
[merge]
	tool = vimdiff
	conflictstyle = merge
[alias]
	co = checkout
	tip = log -1 HEAD
	path = remote -v
	down = pull --rebase
	onup = push
	tags = tag -l
[color]
	ui = true
[mergetool "diffconflicts"]
	cmd = diffconflicts vim $BASE $LOCAL $REMOTE $MERGED
	trustexitcode = false
[mergetool]
	keepBackup = false
[push]
	default = matching
[rerere]
	enabled = true
	autoupdate = true
