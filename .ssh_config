# vim: ft=sshconfig

Host 68k*
    User steve
    HostName 68k.org

Host crutest*
    IdentityFile ~/.ssh/crutest_rsa
    User root

Host *treasure-data.com
    user root
    IdentityFile ~/.ssh/gree.pem

Host ec2*amazonaws.com
    User root
    IdentityFile ~/.ssh/ec2.key

Host rockpunk
    User steve
    Host rockpunk.org

Host kuru
    User kurufoot
    HostName kurufootwear.com

Host github.com
    User git
    ForwardAgent no

Host *
    ForwardX11 no
    ForwardAgent yes
    ProxyCommand none

